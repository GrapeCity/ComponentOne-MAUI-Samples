<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FlexGridExplorer.TransposedGrid"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml">
    <Grid>
        <c1:FlexGrid x:Name="grid" ColumnHeaderFontAttributes="Bold" ColumnHeaderGridLinesVisibility="Vertical" GridLinesVisibility="Vertical">
            <c1:FlexGrid.Behaviors>
                <c1:TransposedGridBehavior/>
            </c1:FlexGrid.Behaviors>
        </c1:FlexGrid>
    </Grid>
</ContentPage>