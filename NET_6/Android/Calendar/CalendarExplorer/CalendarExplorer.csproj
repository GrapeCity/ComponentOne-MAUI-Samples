<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0-android</TargetFramework>
    <SupportedOSPlatformVersion>21</SupportedOSPlatformVersion>
    <OutputType>Exe</OutputType>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationId>com.grapecity.android.CalendarExplorer</ApplicationId>
    <ApplicationVersion>1</ApplicationVersion>
    <ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Resources\drawable\gradient_background.xml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="C1.Android.Calendar">
      <Version>7.0.20233.666</Version>
    </PackageReference>
    <PackageReference Include="C1.Android.Core">
      <Version>7.0.20233.666</Version>
    </PackageReference>
    <PackageReference Include="Xamarin.AndroidX.ConstraintLayout" Version="2.1.4.8" />
    <PackageReference Include="Xamarin.AndroidX.RecyclerView" Version="1.3.2" />
  </ItemGroup>
</Project>