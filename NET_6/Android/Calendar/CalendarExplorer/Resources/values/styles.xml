	<style name="CustomFontStyle">
		<item name="android:fontFamily">@font/courier_new</item>
		<item name="android:textSize">24sp</item>
		<item name="android:textColor">#FF0000</item>
	</style>
	<style name="GreenCalendar" parent="C1CalendarStyle">
		<item name="android:background">#FFEBEBEB</item>
		<item name="android:foreground">#FF000000</item>
		<item name="headerBackground">#FF3D834B</item>
		<item name="headerForeground">#FFFFFFFF</item>
		<item name="headerTextSize">18sp</item>
		<item name="headerTypeface">serif</item>
		<item name="dayOfWeekBackground">#FF63A646</item>
		<item name="dayOfWeekForeground">#FFFFFFFF</item>
		<item name="dayOfWeekFormat">d</item>
		<item name="dayOfWeekTextStyle">italic</item>
		<item name="dayOfWeekTypeface">sans</item>
		<item name="dayOfWeekTextSize">16sp</item>
		<item name="selectionBackground">#FFD2D2D2</item>
		<item name="selectionForeground">#FF000000</item>
		<item name="todayBackground">#FF63A646</item>
		<item name="todayTextStyle">bold</item>
		<item name="todayFontFamily">@font/courier_new</item>
		<item name="dayBorderWidth">1dip</item>
		<item name="dayBorderBrush">#FFF2F2F2</item>
		<item name="adjacentDayForeground">#FFA5A5A3</item>
		<item name="dayOfWeekPadding">4</item>
	</style>
